# Server
PORT=3000
NODE_ENV=development
API_VERSION=v1

# Database
DATABASE_URL=postgresql://dashboard:secret@localhost:5435/dashboard

# Redis
REDIS_URL=redis://localhost:6379

# JWT
JWT_SECRET=your-jwt-secret-change-me
JWT_REFRESH_SECRET=your-refresh-secret-change-me
JWT_ACCESS_EXPIRY=15m
JWT_REFRESH_EXPIRY=7d

# Email (Nodemailer)
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=noreply@dashboard.com
SMTP_PASS=your-smtp-password
EMAIL_FROM=Dashboard <noreply@dashboard.com>

# External APIs
NYC_OPEN_DATA_APP_TOKEN=your-socrata-app-token
LOCATENYC_API_KEY=your-locatenyc-key
PERILPULSE_API_KEY=your-perilpulse-key
ATTOM_API_KEY=your-attom-key
NEARMAP_API_KEY=your-nearmap-key
EAGLEVIEW_API_KEY=your-eagleview-key
LIGHTBOX_API_KEY=your-lightbox-key

# Storage (AWS S3 compatible)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret
S3_BUCKET_NAME=dashboard-reports

# App
CORS_ORIGINS=http://localhost:3001,http://localhost:3002,http://localhost:3003
